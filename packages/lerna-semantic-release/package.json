{
  "name": "lerna-semantic-release",
  "description": "automated semver compliant package publishing, for lerna",
  "author": "Joshua Nelson <jonelson@atlassian.com>, Joscha Feth <jfeth@atlassian.com>",
  "bin": {
    "lerna-semantic-release": "bin/lerna-semantic-release.js"
  },
  "bugs": {
    "url": "https://github.com/atlassian/lerna-semantic-release/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog/"
    }
  },
  "dependencies": {
    "lerna-semantic-release-pre": "^4.1.1",
    "lerna-semantic-release-post": "^5.0.14",
    "lerna-semantic-release-perform": "^4.0.15",
    "lerna-semantic-release-io": "^7.0.0"
  },
  "peerDependencies": {
    "lerna": "^2.0.0-beta.31"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "keywords": [
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version",
    "lerna"
  ],
  "license": "MIT",
  "files": [
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian/lerna-semantic-release.git"
  },
  "scripts": {
    "test": "echo 'no tests'; exit 0"
  },
  "release": {
    "branch": "caribou"
  },
  "version": "9.0.3"
}
